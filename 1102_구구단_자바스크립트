<body>
    <script>

        document.write("<h2>구구단 출력하기(1~9단까지)</h2>");
        let i = 1;
        for (let i = 1; i < 10; i++) {
            for (let j = 1; j < 10; j++) {
                document.write(`${i}*${j} = ${i * j}`)
            }
            document.write("<br>")
        }






    </script>

</body>

</html>
